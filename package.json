{
	"name": "pengi",
	"author": "Krish",
	"version": "1.3.0",
	"packageManager": "yarn@3.4.1",
	"private": true,
	"license": "MIT",
	"main": "dist/Bot.js",
	"imports": {
		"#root/*": "./dist/*.js",
		"#lib/*": "./dist/lib/*.js",
		"#utils/*": "./dist/lib/utils/*.js"
	},
	"type": "module",
	"scripts": {
		"start": "node dist/Bot.js --enable-source-maps",
		"build": "tsc -b src",
		"watch": "yarn tsc-watch -b src --onSuccess \"yarn start\"",
		"lint": "eslint --fix --ext .ts src",
		"format": "prettier --write --loglevel=warn \"src/**/*.{js,ts,json}\"",
		"postinstall": "husky install .github/husky",
		"register": "node dist/register.js"
	},
	"dependencies": {
		"@sapphire/fetch": "^3.0.5",
		"@skyra/env-utilities": "^2.0.1",
		"@skyra/http-framework": "^0.15.5",
		"@skyra/logger": "^2.0.4",
		"colorette": "^2.0.20",
		"discord.js": "^14.25.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.2.0",
		"@commitlint/config-conventional": "^20.2.0",
		"@sapphire/eslint-config": "^5.0.6",
		"@sapphire/prettier-config": "^2.0.0",
		"@sapphire/ts-config": "^5.0.3",
		"@types/node": "^25.0.3",
		"@typescript-eslint/eslint-plugin": "^8.51.0",
		"@typescript-eslint/parser": "^8.51.0",
		"discord-api-types": "0.38.37",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "^3.7.4",
		"pretty-quick": "^4.2.2",
		"tsc-watch": "^7.2.0",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": ">=18.x.x"
	},
	"prettier": "@sapphire/prettier-config",
	"lint-staged": {
		"*.{js,ts}": [
			"eslint --fix --ext ts,js"
		]
	},
	"eslintConfig": {
		"extends": "@sapphire",
		"rules": {
			"@typescript-eslint/no-base-to-string": "off"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"volta": {
		"node": "25.2.1"
	}
}
